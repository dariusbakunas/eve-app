    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "ts-essentials": "^7.0.0",
    "typescript": "^3.9.5",
    "uid-safe": "^2.1.5",
    "winston": "^3.2.1"
  },
  "scripts": {
    "dev": "concurrently --kill-others-on-fail \"nodemon\" \"yarn start:client\"",
    "build": "rm -rf ./build-server/ && yarn schema:types && yarn run build:server && yarn run build:client",
    "gcp-build": "yarn build",
    "build:server": "tsc --project ./tsconfig.server.json",
    "build:client": "react-scripts build && cp -R ./build ./build-server/client && rm -rf ./build",
    "start": "node --max-http-header-size=80000 build-server/server/index.js",
    "start:client": "react-scripts start",
    "test": "TZ=UTC react-scripts test",
    "eject": "react-scripts eject",
    "schema:types": "apollo client:codegen --target=typescript --config=apollo.config.js --outputFlat ./src/__generated__",
    "schema:update": "apollo client:download-schema --endpoint=http://localhost:4000/graphql src/__generated__/schema.graphql",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "analyse": "source-map-explorer build-server/client/static/js/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/__generated__"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.1.2",
    "@storybook/react": "^5.3.19",
    "@types/carbon-components-react": "^7.10.2",
    "@types/carbon__icons-react": "^10.10.0",
    "@types/cookie-session": "^2.0.39",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/express": "^4.17.6",
    "@types/helmet": "^0.0.47",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/lodash": "^4.14.158",
    "@types/morgan": "^1.9.0",
    "@types/owasp-password-strength-test": "^1.3.0",
    "@types/passport": "^1.0.3",
    "@types/passport-auth0": "^1.0.4",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "husky": "^4.2.5",
    "node-sass": "^4.14.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "source-map-explorer": "^2.4.2",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.1"
  }
}
